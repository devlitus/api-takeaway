{
  "require": {
      "slim/slim": "4.*",
      "slim/psr7": "^1.0",
      "ext-json": "*",
      "php-di/php-di": "^6.1",
      "selective/config": "^0.2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "tests/"
    }
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  }
}
